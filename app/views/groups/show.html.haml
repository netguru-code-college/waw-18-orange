%div
  %h2
    = @group.name
.well
  %div
    %h3
      Description:
      = @group.description
  %div
    %h3
      Organizer:
      - @group.organizers.each do |organizer|
        = organizer.username
  %div
    %h3
      Members:
      - @group.members.each do |member|
        = member.username + ','
